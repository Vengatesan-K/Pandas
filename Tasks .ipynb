   "source": [
    "# 2.list comprehension (prime)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 29,
   "id": "0540503e",
   "metadata": {},
   "outputs": [],
   "source": [
    "min=1\n",
    "max=100\n",
    "primes = [num for num in range(min,max) if 0 not in [num%i for i in range(2,int(num/2)+1)]]"
   ]
  },
